CREATE TABLE [Table] (
    [Id]      INT           IDENTITY (1, 1) NOT NULL,
    [Name]    NVARCHAR (20) NOT NULL,
    [Sex]     CHAR (1)      NOT NULL,
    [Birth]   DATE          NOT NULL,
    [Address] VARCHAR (255) NULL,
    PRIMARY KEY CLUSTERED ([Id] ASC)
);

